{
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "$id": "https://www.powsybl.orgopen-rao-crac-schema=crac-v2.5.schema",
  "title": "CRAC",
  "description": "CRAC data for electric grid operation",
  "type": "object",
  "properties": {
    "type": {
      "const": "CRAC"
    },
    "version": {
      "const": "2.5"
    },
    "info": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "instants": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/common/instant.json"
      }
    },
    "ra-usage-limits-per-instant": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/common/ra-usage-limits-per-instant.json"
      }
    },
    "networkElementsNamePerId": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "contingencies": {
      "$ref": "resource:/schemas/common/contingency.json"
    },
    "flowCnecs": {
      "type": "array",
      "items": {
        "items": {
          "$ref": "resource:/schemas/flow-cnec/flow-cnec-v2.4.json"
        }
      }
    },
    "angleCnecs": {
      "type": "array",
      "items": {
        "items": {
          "$ref": "resource:/schemas/common/angle-cnec.json"
        }
      }
    },
    "voltageCnecs": {
      "type": "array",
      "items": {
        "items": {
          "$ref": "resource:/schemas/common/voltage-cnec.json"
        }
      }
    },
    "pstRangeActions": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/pst-range-action/pst-range-action-v2.4.json"
      }
    },
    "hvdcRangeActions": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/hvdc-range-action/hvdc-range-action-v2.4.json"
      }
    },
    "injectionRangeActions": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/injection-range-action/injection-range-action-v2.4.json"
      }
    },
    "counterTradeRangeActions": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/counter-trade-range-action/counter-trade-range-action-v2.4.json"
      }
    },
    "networkActions": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/network-action/network-action-v2.5.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "type",
    "version",
    "id",
    "name"
  ]
}

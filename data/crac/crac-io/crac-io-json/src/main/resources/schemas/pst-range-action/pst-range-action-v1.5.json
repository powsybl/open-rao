{
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "$id": "https://www.powsybl.org?open-rao-crac-schema=pst-range-action-v1.5.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": [
        "string",
        "null"
      ]
    },
    "operator": {
      "type": [
        "string",
        "null"
      ]
    },
    "groupId": {
      "type": [
        "string",
        "null"
      ]
    },
    "speed": {
      "type": "number"
    },
    "networkElementId": {
      "type": "string"
    },
    "initialTap": {
      "type": "number"
    },
    "tapToAngleConversionMap": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "ranges": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "rangeType": {
            "$ref": "resource:/schemas/range-type/range-type-v1.0.json"
          },
          "max": {
            "type": "number"
          },
          "min": {
            "type": "number"
          }
        },
        "additionalProperties": false,
        "required": [
          "rangeType"
        ]
      }
    },
    "freeToUseUsageRules": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/common/free-to-use-usage-rule.json"
      }
    },
    "onStateUsageRules": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/common/on-state-usage-rule.json"
      }
    },
    "onFlowConstraintUsageRules": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/on-flow-constraint-usage-rule/on-flow-constraint-usage-rule-v1.0.json"
      }
    },
    "onAngleConstraintUsageRules": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/on-angle-constraint-usage-rule/on-angle-constraint-usage-rule-v1.4.json"
      }
    },
    "onFlowConstraintInCountryUsageRules": {
      "type": "array",
      "items": {
        "$ref": "resource:/schemas/on-flow-constraint-in-country-usage-rule/on-flow-constraint-in-country-usage-rule-v1.3.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "networkElementId",
    "initialTap",
    "tapToAngleConversionMap",
    "ranges"
  ]
}
